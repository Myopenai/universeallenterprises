{
  "version": "1.0.0",
  "enabled": true,
  "algorithm": "AES-256-GCM",
  "keyDerivation": "PBKDF2",
  "iterations": 100000,
  "saltLength": 32,
  "ivLength": 16,
  "tagLength": 16,
  "tttSymbolic": {
    "enabled": true,
    "publicKey": "T,.",
    "privateKey": "T,,.",
    "algorithm": "T,.&T,,.",
    "description": "T,. (Punkt) = Public Key / Start, T,,. (Komma) = Private Key / Continuation"
  },
  "keyManagement": {
    "storage": "encrypted",
    "rotation": true,
    "rotationInterval": 90,
    "backup": true
  },
  "settings": {
    "encryptAtRest": true,
    "encryptInTransit": true,
    "requireAuth": true
  }
}








