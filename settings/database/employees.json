{
  "version": "1.0.0",
  "lastUpdated": "2025-11-25",
  "onboarding": {
    "enabled": true,
    "autoWelcome": true,
    "autoAccess": true,
    "autoNaming": true,
    "welcomeMessage": "Herzlich willkommen bei Together Systems & Startup Systems! Wir freuen uns, dass Sie Teil unseres Teams sind.",
