    }
  },
  "verification": {
    "notary": {
      "enabled": true,
      "required": false,
      "cost": 0,
      "onDemand": true
    },
    "digital": {
      "enabled": true,
      "required": true,
      "method": "email"
    }
  },
  "integration": {
    "togetherSystems": true,
    "startupSystems": true,
    "github": true,
    "cloudflare": true,
    "buildTools": true
  }
}








