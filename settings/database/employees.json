  },
  "employees": [],
  "templates": {
    "welcome": {
      "subject": "Willkommen bei Together Systems & Startup Systems",
      "message": "Herzlich willkommen! Sie wurden erfolgreich als Mitarbeiter registriert. Ihr Zugang wurde automatisch eingerichtet.",
      "include": [
        "accessCredentials",
        "projectLinks",
        "documentation",
        "supportContacts"
      ]
    },
    "naming": {
      "pattern": "{firstName}.{lastName}",
      "lowercase": true,
      "specialChars": false
    },
    "access": {
      "defaultRole": "developer",
      "autoPermissions": [
        "read",
        "write",
        "test",
        "deploy"
      ],
