{
  "version": "1.0.0",
  "name": "project-template",
  "description": "Projektunabhängiges Template für neue Projekte",
  "type": "template",
  "settings": {
    "projectType": "static",
    "framework": "vanilla",
    "buildTool": "none",
    "testFramework": null,
    "deployment": ["manual"],
    "features": []
  },
  "config": {
    "mcp": {
      "enabled": true
    },
    "playwright": {
      "enabled": false
    },
    "autofix": {
      "enabled": true
    },
    "deployment": {
      "provider": "cloudflare-pages",
      "autoDeploy": false
    },
    "neuralNetwork": {
      "enabled": false
    },
    "encryption": {
      "enabled": true,
      "tttSymbolic": true
    }
  },
  "structure": {
    "files": [
      "package.json",
      "README.md",
      ".gitignore"
    ],
    "directories": [
      "src",
      "public",
      "tests"
    ]
  },
  "instructions": {
    "setup": [
      "1. Kopiere Template in neues Projekt",
      "2. Passe package.json an",
      "3. Lade Settings mit loadSettings()",
      "4. Konfiguriere Deployment",
      "5. Starte Entwicklung"
    ]
  }
}








