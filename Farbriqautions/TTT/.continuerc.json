      }
    },
    {
      "title": "Mistral (Ollama Local)",
      "provider": "ollama",
      "model": "mistral",
      "apiBase": "http://localhost:11434",
      "contextLength": 8192,
      "completionOptions": {
        "temperature": 0.2,
        "maxTokens": 4096
      }
