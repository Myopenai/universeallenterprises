  ],
  "tabAutocompleteModel": {
    "title": "DeepSeek Coder Autocomplete",
    "provider": "ollama",
    "model": "deepseek-coder",
    "apiBase": "http://localhost:11434",
    "completionOptions": {
      "temperature": 0.0,
      "maxTokens": 256
    }
  },
  "embeddingsProvider": {
    "provider": "ollama",
    "model": "nomic-embed-text",
    "apiBase": "http://localhost:11434"
  },
  "customCommands": [
    {
      "name": "fix",
      "description": "Auto-Fix: Clean, Build, Hash, Deploy",
      "prompt": "Führe das Auto-Fix-System aus: 1) Clean Build-Verzeichnis, 2) Deterministischer Build, 3) Asset-Hashing für Cache-Busting, 4) Service Worker Version Bump. Zeige die Befehle und erkläre jeden Schritt."
    },
    {
      "name": "deploy",
      "description": "Deploy-Anleitung für TogetherSystems",
      "prompt": "Erstelle eine Deploy-Anleitung für das TogetherSystems-Projekt auf GitHub Pages. Zeige: git add, commit, push Befehle mit korrektem Branch und Workflow."
    },
    {
      "name": "analyze",
      "description": "Code-Analyse für Stabilitätsprobleme",
      "prompt": "Analysiere den Code auf potenzielle Stabilitätsprobleme: Cache-Probleme, IDE-Abhängigkeiten, nicht-deterministische Builds. Gib konkrete Lösungen."
    }
  ],
  "slashCommands": [
    {
      "name": "strict",
      "description": "Strikte Projekt-Regeln aktivieren"
    },
    {
      "name": "build",
      "description": "Build-Befehle anzeigen"
    },
    {
      "name": "cache",
      "description": "Cache-Busting Strategie"
    }
  ],
  "contextProviders": [
    {
      "name": "code",
      "params": {}
    },
    {
      "name": "docs",
      "params": {}
    },
    {
      "name": "terminal",
      "params": {}
    },
    {
      "name": "problems",
      "params": {}
    },
    {
      "name": "folder",
      "params": {}
    }
  ],
  "allowAnonymousTelemetry": false,
  "systemMessage": "Du bist ein präziser Coding-Assistent für das TogetherSystems-Projekt [.TTT T,.&T,,.T,,,.T.]. REGELN: 1) Befolge Anweisungen exakt, ändere Architektur nur auf explizite Anfrage. 2) Verwende nur Libraries aus dem Repo/Lockfile. 3) Schreibe kompilierbaren, getesteten Code. 4) Halte dich an Build-Skripte, Makefile und CI. 5) Bei Unsicherheit: frage zuerst. 6) Deterministische Antworten, keine Eigenwilligkeit."
}

