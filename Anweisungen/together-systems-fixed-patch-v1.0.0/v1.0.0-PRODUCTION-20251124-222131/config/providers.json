{
  "version": "v1",
  "providers": [
    {
      "id": "public.manifest.portal",
      "label": "Öffentliches Manifest-Portal",
      "description": "Öffentlicher Feed für Beiträge aus dem Offline-Manifest (HTTP/JSON).",
      "category": "public",
      "connector": "http",
      "priority": 10,
      "endpoint": "/api/manifest/submit",
      "mode": "broadcast",
      "security": "tls",
      "tags": ["portal", "manifest", "feed"],
      "meta": {
        "method": "POST",
        "expectsJson": true
      },
      "enabled": true
    },
    {
      "id": "presence.auto_connect",
      "label": "Direktverbinden (Auto-Connect)",
      "description": "Auto-Matching von Thinkern über Präsenz-Backend und WebSocket-Raum.",
      "category": "system",
      "connector": "webrtc",
      "priority": 5,
      "endpoint": "/api/presence",
      "mode": "direct",
      "security": "tls",
      "tags": ["presence", "room", "webrtc"],
      "meta": {
        "heartbeatPath": "/heartbeat",
        "matchPath": "/match",
        "wsSignalingUrl": "wss://ts-portal.pages.dev/ws"
      },
      "enabled": true
    },
    {
      "id": "business.auto.dealer.demo",
      "label": "Autohaus – Kurzfristige Mobilität (Demo)",
      "description": "Beispiel-Profil für hochwertige Fahrzeugangebote mit fairen, günstigen Konditionen.",
      "category": "private",
      "connector": "http",
      "priority": 50,
      "endpoint": "/api/business/auto-offers",
      "mode": "broadcast",
      "security": "tls",
      "tags": ["auto", "mobility", "offers"],
      "meta": {
        "method": "POST",
        "expectsJson": true
      },
      "enabled": false
    }
  ]
}


